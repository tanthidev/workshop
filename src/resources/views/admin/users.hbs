<div id="edit" class="absolute hidden h-screen  w-screen bg-gray-900 bg-opacity-80 top-0 left-0 z-20">
    {{!-- Form --}}
    <form id="formEdit" action="/user/profile" method="POST" class="grid w-1/3 ml-auto bg-white h-screen ">
                    <div class="relative">
                        <div onclick="hiddenEdit()" class="absolute left-5 top-5 cursor-pointer">
                            <img src="https://icon-library.com/images/x-icon-vector/x-icon-vector-27.jpg" alt="" width="35" height="35">
                        </div>
                    </div>

                    <div class="p-12">
                        {{!-- _id --}}
                        <input 
                            id="_id"
                            type="text"
                            name="_id"
                            value=""
                            class="hidden"
                            readonly
                        >
                        {{!-- Fullname --}}
                        <div class="items-center mt-8 mb-3 text-[#202142]">
                                <div class="w-full">
                                    <label for="full_name"
                                        class="block mb-2 text-sm font-medium text-indigo-900 ">Full name</label>
                                    <input type="text" id="full_name"
                                        name="full_name"
                                        class="bg-indigo-50 border border-indigo-300 text-indigo-900 text-sm rounded-lg focus:ring-indigo-500 focus:border-indigo-500 block w-full p-2.5 "
                                        placeholder="Full name" 
                                        value="" required>
                                </div>

                        </div>

                        {{!-- Email --}}
                        <div class="mb-3">
                                <label for="email"
                                    class="block mb-2 text-sm font-medium text-indigo-900 ">Email</label>
                                <input type="email" id="email"
                                    name="email"
                                    class="bg-indigo-50 border border-indigo-300 text-indigo-900 text-sm rounded-lg focus:ring-indigo-500 focus:border-indigo-500 block w-full p-2.5 "
                                    placeholder="your.email@mail.com" 
                                    value="" required>
                        </div>

                        {{!-- Phone --}}
                        <div class="mb-3">
                                <label for="phone"
                                    class="block mb-2 text-sm font-medium text-indigo-900 ">Phone</label>
                                <input type="phone" id="phone"
                                    name="phone"
                                    class="bg-indigo-50 border border-indigo-300 text-indigo-900 text-sm rounded-lg focus:ring-indigo-500 focus:border-indigo-500 block w-full p-2.5 "
                                    placeholder="Your phone number" 
                                    value="" required>
                        </div>
                        
                        {{!-- Role --}}
                        <div class="mb-3">
                            <label for="role"
                                class="block mb-2 text-sm font-medium text-indigo-900 ">Role</label>
                            <select id="role" name="role" class="bg-indigo-50 border border-indigo-300 text-indigo-900 text-sm rounded-lg focus:ring-indigo-500 focus:border-indigo-500 block w-full p-2.5">
                                <option 
                                    value="participant"
