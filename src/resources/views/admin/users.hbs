</div>



<script>
    
    const editElement = document.getElementById('edit');
    const formEdit = document.getElementById('formEdit');
    function edit(user){
        const parsedData = JSON.parse(user);
        
        const _id = document.getElementById('_id');
        const full_name = document.getElementById('full_name');
        const email = document.getElementById('email');
        const phone = document.getElementById('phone');
        const role = document.getElementById('role');
        const gender = document.getElementById('gender');
        const birhday = document.getElementById('birhday');

        _id.value = parsedData._id
        full_name.value = parsedData.full_name
        email.value = parsedData.email
        phone.value = parsedData.phone
        role.value = parsedData.role
        gender.value = parsedData.gender



        showEdit()
    }

    function showEdit(){
        editElement.classList.remove('hidden')
    }

    function hiddenEdit(){
        editElement.classList.add('hidden')
    }

    // Thêm sự kiện "click" cho phần tử 1
        editElement.addEventListener('click', function(event) {
            // Kiểm tra xem phần tử mà người dùng nhấp vào có phải là phần tử 1 hay không
            // Nếu không phải phần tử 2, ẩn đi phần tử 1
            if (event.target !== formEdit && !formEdit.contains(event.target)) {
                hiddenEdit()
            }
        });
</script>